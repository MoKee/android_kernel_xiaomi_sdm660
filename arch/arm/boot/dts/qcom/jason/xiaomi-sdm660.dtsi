#include "jason-sdm660-mdss-panels.dtsi"

// msm-pm660.dtsi
&spmi_bus {
	qcom,pm660@0 {
		qcom,power-on@800 {
			qcom,pon_1 {
				qcom,support-reset = <1>;
				qcom,s1-timer = <4480>;
				qcom,s2-timer = <2000>;
				qcom,s2-type = <7>;
			};

			qcom,pon_3 {
				qcom,pon-type = <3>;
				qcom,support-reset = <1>;
				qcom,pull-up = <1>;
				qcom,s1-timer = <1352>;
				qcom,s2-timer = <2000>;
				qcom,s2-type = <1>;
				qcom,use-bark;
			};
		};
	};
};

&pm660_pdphy {
	qcom,default-sink-caps = <5000 3000>, /* 5V @ 3A */
				<9000 2000>; /* 9V @ 2A */
};

&pm660_haptics {
	qcom,lra-res-cal-period = <4>;
	qcom,misc-trim-error-rc19p2-clk-reg-present;
};

// msm-pm660l.dtsi
&pm660l_flash0 {
	qcom,ires-ua = <13000>;
};

&pm660l_flash1 {
	qcom,ires-ua = <13000>;
};

&pm660l_torch0 {
	qcom,max-current = <200>;
	qcom,current-ma = <100>;
	qcom,ires-ua = <13000>;
};

&pm660l_torch1 {
	qcom,max-current = <200>;
	qcom,current-ma = <100>;
	qcom,ires-ua = <13000>;
};

// sdm660-camera.dtsi
&i2c_freq_400Khz {
	qcom,hw-scl-stretch-en = <1>;
};

// sdm660-mdss.dtsi
&mdss_dsi0 {
	wqhd-vddio-supply = <&disp_vdd_vreg>;
};

// sdm660-pinctrl.dtsi
&i2c_4_sleep {
	config {
		/delete-property/ bias-pull-up;
		bias-disable;
	};
};

&nfc_int_active {
	mux {
		/* GPIO 48 NFC Read Interrupt */
		pins = "gpio48";
	};

	config {
		pins = "gpio48";
	};
};

&nfc_int_suspend {
	mux {
		/* GPIO 48 NFC Read Interrupt */
		pins = "gpio48";
	};

	config {
		pins = "gpio48";
	};
};

&nfc_enable_active {
	mux {
		/* 65: NFC ENABLE */
		/* 45:ESE Enable */
		pins = "gpio65", "gpio45";
	};

	config {
		pins = "gpio65", "gpio45";
	};
};

&nfc_enable_suspend {
	mux {
		/* 65: NFC ENABLE */
		/* 45:ESE Enable */
		pins = "gpio65", "gpio45";
	};

	config {
		pins = "gpio65", "gpio45";
	};
};

&wcd_gnd_mic_swap_idle {
	mux {
		pins = "gpio56";
	};
	config {
		pins = "gpio56";
	};
};

&wcd_gnd_mic_swap_active {
	mux {
		pins = "gpio56";
	};
	config {
		pins = "gpio56";
	};
};

&tlmm {
	cam_sub_actuator_vaf_active: cam_sub_actuator_vaf_active {
		/* ACTUATOR POWER */
		mux {
			pins = "gpio44";
			function = "gpio";
		};

		config {
			pins = "gpio44";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sub_actuator_vaf_suspend: cam_sub_actuator_vaf_suspend {
		/* ACTUATOR POWER */
		mux {
			pins = "gpio44";
			function = "gpio";
		};

		config {
			pins = "gpio44";
			bias-pull-down; /* PULL DOWN */
			drive-strength = <2>; /* 2 MA */
		};
	};
};

