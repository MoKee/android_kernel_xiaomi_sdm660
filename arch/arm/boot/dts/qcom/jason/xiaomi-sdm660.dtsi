			};
		};
			gpio_rf_suspend_c8: gpio_rf_suspend_c8 {
			mux {
				pins = "gpio104";
				function = "gpio";
			};
			config {
				pins = "gpio104";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};
};

